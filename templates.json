[
    {
      "title": "Filestash",
      "name": "filestash",
      "image": "machines/filestash",
      "categories": [
        "Admin"
      ],
      "platform": "linux",
      "restart_policy": "unless-stopped",
      "ports": [
        "8334:8334/tcp"
      ],
      "network_mode": "bridge",
